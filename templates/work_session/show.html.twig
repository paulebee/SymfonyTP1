{% extends 'base.html.twig' %}

{% block title %}WorkSession{% endblock %}

{% block body %}
    <h1>WorkSession</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ work_session.id }}</td>
            </tr>
            <tr>
                <th>StartedAt</th>
                <td>{{ work_session.startedAt ? work_session.startedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>FinishedAt</th>
                <td>{{ work_session.finishedAt ? work_session.finishedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_work_session_index') }}">back to list</a>

{% endblock %}
